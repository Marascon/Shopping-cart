{
  "name": "marascon/shopping-cart",
  "description": "A shopping cart",
  "type": "library",
  "license": "MIT",
  "version": "1.0.1",
  "authors": [
    {
      "name": "Gert-Jan Scholten",
      "email": "admin@marascon.com"
    }
  ],
  "require": {
    "php": ">=8.3"
  },
  "autoload": {
    "psr-4": {
      "Marascon\\ShoppingCart\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Marascon\\ShoppingCart\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "scripts": {
    "test": "phpunit --color=always --bootstrap vendor/autoload.php --testdox  tests/",
    "list-tests": "phpunit --list-tests tests/"
  }
}
